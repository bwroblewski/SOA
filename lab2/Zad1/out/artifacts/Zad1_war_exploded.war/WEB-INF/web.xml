<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>zadGetServ</servlet-name>
        <servlet-class>zadGetServ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>zadGetServ</servlet-name>
        <url-pattern>/zadGetServ</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>zadPost</servlet-name>
        <servlet-class>zadPost</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>zadPost</servlet-name>
        <url-pattern>/zadPost</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>zadPostServ</servlet-name>
        <servlet-class>zadPostServ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>zadPostServ</servlet-name>
        <url-pattern>/zadPostServ</url-pattern>
    </servlet-mapping>

</web-app>